{
  "compilerOptions": {
    "lib": [
      "es2017"
    ],
    "outDir": "dist",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "declaration": true,
    "esModuleInterop": true,
    "target": "es2020",
    "module": "CommonJS",
  },
  "include": [
    "src/**/*"
  ],
  "exclude": []
}